{
  "version": 2,
  "buildCommand": "npm install",
  "outputDirectory": "./",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"],
  "functions": {
    "index.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
